   "source": [
    "n = int(input(\"請輸入十進制: \"))\n",
    "b = int(input(\"請輸入欲轉換進制: \"))\n",
    "\n",
    "def conv(n,b):\n",
    "    convertString = \"0123456789ABCDEFGHIJKLMNOP\"\n",
    "    \n",
    "    if n < b:\n",
    "        return convertString[n]\n",
    "    else:\n",
    "        return conv(n//b,b) + convertString[n%b]\n",
    "    \n",
    "print(conv(n,b))"
   ]
